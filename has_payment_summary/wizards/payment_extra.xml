<?xml version="1.0" ?>
<odoo>

	<record
		id="view_account_payment_register_form_extends"
		model="ir.ui.view">
		<field name="name">view.account.payment.register.form.extends
		</field>
		<field name="model">account.payment.register</field>
		<field name="inherit_id" ref="account.view_account_payment_register_form" />
		<field name="arch" type="xml">
			<xpath expr="//group/group[@name='group1']/field[@name='journal_id']" position="after">
				<field name="material" invisible="payment_type != 'inbound'"/>
                <field name="consultoria" widget="boolean_toggle" invisible="payment_type != 'inbound'"/>
				<field name="journal_type" invisible="1"/>
			</xpath>
		</field>
	</record>
	
</odoo>
